
pipeline {
    agent {
        label 'lms-slavenode'
    }
    environment {
        DOCKERHUB_CREDENTIALS = credentials('dockerhub-credentials')
        registry = "yoursureshdockerhub/lms-backend"
        registryCredential = 'yoursureshdockerhub'
        dockerImage = ''
    }
    stages {
        stage('update server') {
            steps {
                sh 'sudo apt update -y'
            }
        }
    }
}
